# LangGraph Configuration
NEXT_PUBLIC_API_URL=http://localhost:2024
NEXT_PUBLIC_ASSISTANT_ID=agent
# Do NOT prefix this with "NEXT_PUBLIC_" as we do not want this exposed in the client.
LANGSMITH_API_KEY=

# GCS bucket used for file uploads
GCS_BUCKET_NAME=

# Model provider selection
# Server-side value controls server behavior
MODEL_PROVIDER=OPENAI
# Client-side hint for building content blocks
NEXT_PUBLIC_MODEL_PROVIDER=OPENAI

# Optional: OpenAI Files API for PDFs (when MODEL_PROVIDER=OPENAI)
OPENAI_API_KEY=
# Purpose and expiry settings for OpenAI files
OPENAI_FILES_PURPOSE=assistants
# expires_after[anchor] can be: created_at
OPENAI_FILES_EXPIRES_AFTER_ANCHOR=created_at
# expires_after[seconds] default ~90 days (7776000)
OPENAI_FILES_EXPIRES_AFTER_SECONDS=7776000

# Production LangGraph Configuration (quickstart) - Uncomment to use
# NEXT_PUBLIC_ASSISTANT_ID="agent"
# This should be the deployment URL of your LangGraph server
# LANGGRAPH_API_URL="https://my-agent.default.us.langgraph.app"
# This should be the URL of your website + "/api". This is how you connect to the API proxy
# NEXT_PUBLIC_API_URL="https://my-website.com/api"
# LANGSMITH_API_KEY="lsv2_..."
